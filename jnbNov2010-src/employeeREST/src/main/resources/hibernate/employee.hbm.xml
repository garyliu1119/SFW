<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping default-lazy="false">
  
   <class name="com.ociweb.corespring.examples.employee.domain.Employee" table="t_employee">
       <id name="id" type="long" unsaved-value="null">
           <generator class="identity"/>
       </id>
       <property name="firstName" column="First_Name" not-null="true"/>
       <property name="lastName" column="Last_Name" not-null="true"/>
       <property name="address" column="Address" not-null="true"/>
       <property name="city" column="City" not-null="true"/>
       <property name="state" column="State" not-null="true"/>
       <property name="jobTitle" column="Title" not-null="true"/>
       <property name="hireDate" column="Hire_Date" not-null="true"/>
       <property name="zip" column="Zip" not-null="true"/>
   </class>
  
</hibernate-mapping>
